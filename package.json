    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmdoc/node-npmdoc-empty-trash",
    "keywords": [
        "documentation",
        "empty-trash"
    ],
    "license": "MIT",
    "main": "lib.npmdoc_empty_trash.js",
    "name": "npmdoc-empty-trash",
    "nameAlias": "npmdoc_empty_trash",
    "nameOriginal": "npmdoc-empty-trash",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmdoc/node-npmdoc-empty-trash.git"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
